(2) s->p2:x{x >= 0 /\ x < 4 /\ sc' = x /\ sa' = sa /\ sb' = sb} (3)
(3) p0->p1:x{x = sa /\ sb' = x /\ sa' = sa /\ sc' = sc} (4)
(4) p1->p2:x{x = sb /\ sc' = x /\ sa' = sa /\ sb' = sb} (5)
(5) p2->p0:x{x = sc /\ ((x = sa /\ sa' = (sa + 1) % 4) \/ (x != sa /\ sa' = sa)) /\ sb' = sb /\ sc' = sc} (3)
